-- لیستی بەکارهێنەرە بلۆک کراوەکان
list = {'Testbandscript', 'user2', 'Qween_Rozhe', 'exampleName'}

-- فەرمانی پشکنین بۆ بلۆک کردنی بەکارهێنەر
local function checkUser()
    local player = game.Players.LocalPlayer
    local playerName = player.Name
    
    -- بگەڕێ بە لیستەکە و بزانە ئایا ناوی بەکارهێنەر لە لیستەکەیە
    for _, blockedUser in ipairs(list) do
        if playerName == blockedUser then
            -- دەنگ
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://1301186018"
            sound.Parent = game:GetService("SoundService")
            sound:Play()
            
            -- ئاگاداری
            game.StarterGui:SetCore("SendNotification", {
                Title = "Kurd Hub",
                Text = "تۆ لە لیستی بلۆک کراوەکانیت!",
                Duration = 5
            })
            
            wait(3.5)
            player:Kick("تۆ لە لیستی بلۆک کراوەکانیت لە Kurd Hub!")
            
            -- بوەستێنە کۆدی تر
            while true do end
        end
    end
end

-- یەکەم جار پشکنین بکە
checkUser()

-- پشکنینی هەموو 5 چرکە جارێک
while true do
    wait(5)
    checkUser()
end





