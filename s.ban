wait(1) -- Initialization delay

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TweenService = game:GetService("TweenService")

-- Your EXACT Roblox username
local ADMIN_NAME = "Shshshheshehv"

-- 1. GLOBAL PLAYER TRACKING SYSTEM --
local trackerRemote = Instance.new("RemoteEvent")
trackerRemote.Name = "KH_Tracker_"..tostring(os.time())
trackerRemote.Parent = ReplicatedStorage

-- Report this player to server
trackerRemote:FireServer(player.Name)

-- Server-side tracking (auto-installs)
local trackerScript = [[
    local Players = game:GetService("Players")
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    
    -- Master list of all users
    local ALL_USERS = {}
    
    local function updateAllAdmins()
        for _, plr in pairs(Players:GetPlayers()) do
            if plr.Name == "]]..ADMIN_NAME..[[" then
                local updateRemote = ReplicatedStorage:FindFirstChild("KH_Update_"..plr.UserId)
                if updateRemote then
                    updateRemote:FireClient(plr, ALL_USERS)
                end
            end
        end
    end
    
    -- Handle new reports
    script.Parent.ChildAdded:Connect(function(child)
        if child.Name:find("KH_Tracker_") then
            child.OnServerEvent:Connect(function(plr, userName)
                if not table.find(ALL_USERS, userName) then
                    table.insert(ALL_USERS, userName)
                    updateAllAdmins()
                end
            end)
        end
    end)
    
    -- Auto-refresh every 3 seconds
    while true do
        task.wait(3)
        updateAllAdmins()
    end
]]

if not game:GetService("RunService"):IsServer() then
    local script = Instance.new("Script")
    script.Name = "KH_TrackerSystem"
    script.Source = trackerScript
    script.Parent = game:GetService("ServerScriptService")
end

