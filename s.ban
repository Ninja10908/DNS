-- Track all users who executed this script
local scriptUsers = {}
table.insert(scriptUsers, player.Name)

-- 1. STEALTH NOTIFICATION FOR NORMAL PLAYERS
if player.Name ~= ADMIN_NAME then
    local gui = Instance.new("ScreenGui")
    gui.Name = "StealthNotify"
    gui.Parent = player.PlayerGui
    
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 200, 0, 40)
    frame.Position = UDim2.new(1, -210, 1, -50)
    frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    frame.BackgroundTransparency = 0.5
    frame.Parent = gui
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, -10, 1, -10)
    label.Position = UDim2.new(0, 5, 0, 5)
    label.Text = "âœˆï¸ Flight system initialized"
    label.TextColor3 = Color3.fromRGB(255, 255, 255)
