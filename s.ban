    -- Toggle visibility
    local panelVisible = false
    local function togglePanel()
        panelVisible = not panelVisible
        mainFrame.Visible = panelVisible
        
        if panelVisible then
            toggleButton.Text = "üîΩ HIDE CONTROLS"
            mainFrame:TweenPosition(
                UDim2.new(0.5, -175, 0.5, -150),
                Enum.EasingDirection.Out,
                Enum.EasingStyle.Quad,
                0.3,
                true
            )
        else
            toggleButton.Text = "üîº ADMIN CONTROLS"
            mainFrame:TweenPosition(
                UDim2.new(0.5, -175, 0.5, -400),
                Enum.EasingDirection.In,
                Enum.EasingStyle.Quad,
                0.3,
                true
            )
        end
    end
    
    toggleButton.MouseButton1Click:Connect(togglePanel)
    
    -- Update user list with auto-refresh
    local function updateUserList(users)
        userList:ClearAllChildren()
        title.Text = "üëë KURD HUB ADMIN ("..#users.." users)"
        
        for _, userName in pairs(users) do
            local userFrame = Instance.new("Frame")
            userFrame.Size = UDim2.new(1, 0, 0, 30)
            userFrame.BackgroundTransparency = 1
            
            local nameLabel = Instance.new("TextLabel")
            nameLabel.Size = UDim2.new(0.7, 0, 1, 0)
            nameLabel.Text = "‚Ä¢ "..userName
            nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            nameLabel.BackgroundTransparency = 1
            nameLabel.Font = Enum.Font.SourceSans
            nameLabel.TextSize = 16
            nameLabel.TextXAlignment = Enum.TextXAlignment.Left
            nameLabel.Parent = userFrame
            
            local kickBtn = Instance.new("TextButton")
            kickBtn.Size = UDim2.new(0.25, 0, 0.8, 0)
            kickBtn.Position = UDim2.new(0.75, 0, 0.1, 0)
            kickBtn.Text = "Kick"
            kickBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
            kickBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
            kickBtn.Font = Enum.Font.SourceSansBold
            kickBtn.TextSize = 14
            kickBtn.Parent = userFrame
            
            kickBtn.MouseButton1Click:Connect(function()
                kickRemote:FireServer(userName)
                local target = Players:FindFirstChild(userName)
                if target then
                    target:Kick("üîá Kicked by admin")
                end
            end)
            
            userFrame.Parent = userList
        end
    end
    
    -- Receive updates from server (auto-refreshes every 3 seconds)
    updateRemote.OnClientEvent:Connect(updateUserList)
    
    -- Kick functionality
    local function kickPlayer(targetName)
        local target = Players:FindFirstChild(targetName)
        if target then
            kickRemote:FireServer(targetName)
            target:Kick("üîá Kicked by admin")
            return true
        end
        return false
    end
    
    kickButton.MouseButton1Click:Connect(function()
        local targetName = kickBox.Text
        if kickPlayer(targetName) then
            kickBox.Text = ""
            local original = kickButton.Text
            kickButton.Text = "‚úÖ KICKED!"
            task.wait(1.5)
            kickButton.Text = original
        else
            local original = kickButton.Text
            kickButton.Text = "‚ùå NOT FOUND!"
            task.wait(1.5)
            kickButton.Text = original
        end
    end)
    
    -- Chat command handler
    player.Chatted:Connect(function(msg)
        msg = string.lower(string.gsub(msg, "%s+", " "))
        if string.sub(msg, 1, 5) == "/kick" then
            local targetName = string.match(msg, "^/kick%s+(%S+)")
            if targetName then
                kickPlayer(targetName)
            end
        end
    end)
    
    -- Server-side kick (auto-installs)
    local serverScript = [[
        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        
        -- Handle kick requests
        script.Parent.ChildAdded:Connect(function(child)
            if child.Name:find("KH_KickSystem_") then
                child.OnServerEvent:Connect(function(plr, targetName)
                    if plr.Name == "]]..ADMIN_NAME..[[" then
                        local target = Players:FindFirstChild(targetName)
                        if target then
                            target:Kick("üîá Kicked by admin")
                        end
                    end
                end)
            end
        end)
    ]]
    
    if not game:GetService("RunService"):IsServer() then
        local script = Instance.new("Script")
        script.Name = "KH_KickProcessor"
        script.Source = serverScript
        script.Parent = game:GetService("ServerScriptService")
    end
    
    -- Request initial data
    trackerRemote:FireServer("REQUEST_DATA")
end





